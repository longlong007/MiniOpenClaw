{
  "name": "@mini-openclaw/gateway",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mini-openclaw/core": "workspace:*",
    "express": "^4.19.0",
    "ws": "^8.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.12",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.0.0",
    "tsup": "^8.2.0",
    "typescript": "^5.5.0"
  }
}
